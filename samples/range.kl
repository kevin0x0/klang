local next(next, end) => {
  if next >= end: return;
  return next + 1, end, next;
};

let Range = {
  local begin;
  local end;
  constructor = method(begin, end) => {
    this.begin = begin;
    this.end = end;
  };
  '<-' = method => {
    if this.begin >= this.end: return;
    return next, this.begin + 1, this.end, this.begin;
  }
};

local range n -> new Range(0, n);

for i in range 100:
  print i;
