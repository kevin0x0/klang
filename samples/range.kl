local next(next, end) => {
  if next >= end: return;
  return next + 1, end, next;
};

let Range = {
  local begin;
  local end;
  '<-' = method => {
    if this.begin == this.end: return;
    return next, this.begin + 1, this.end, this.begin;
  }
};

local range n => {
  return obj where {
    let obj = new Range;
    obj.begin = 0;
    obj.end = n;
  };
};

for i in range 100:
  print i;
